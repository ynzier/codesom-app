{
  "expo": {
    "name": "CodesomPOSTest(iOS)",
    "slug": "codesom-ipad-test",
    "owner": "miloteam",
    "version": "1.0.3",
    "orientation": "landscape",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y",
      "requireFullScreen": true
    },
    "android": {
      "package": "com.miloteam.codesom",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
